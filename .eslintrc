{
    "parser": "@babel/eslint-parser",
    "env": {
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    },
    "ignorePatterns": ["doc", "dist"]
}
