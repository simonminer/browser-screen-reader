{
    "name": "site-unseen",
    "version": "1.0.0",
    "description": "Screen reader simulator for web browsers",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "server": "./node_modules/http-server/bin/http-server ./src -c-1",
        "lint": "eslint .",
        "jsdoc": "./node_modules/jsdoc/jsdoc.js -a all -c jsdoc.json -r -R README.md src/modules/ -d doc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/simonminer/site-unseen.git"
    },
    "author": "Simon Miner",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/simonminer/site-unseen/issues"
    },
    "homepage": "https://siteunseen.dev",
    "dependencies": {
        "@babel/preset-env": "7.13.12",
        "axe-core": "4.1.3",
        "browserslist": "4.18.1",
        "caniuse-lite": "1.0.30001204",
        "jest-cli": "26.6.3"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "jest": {
        "transform": {
            ".*": "<rootDir>/node_modules/babel-jest"
        },
        "unmockedModulePathPatterns": [
            "<rootDir>/node_modules/fbjs"
        ]
    },
    "devDependencies": {
        "@babel/core": "7.13.13",
        "@babel/eslint-parser": "7.16.3",
        "babel-jest": "26.6.3",
        "babel-plugin-transform-class-properties": "6.24.1",
        "docdash": "1.2.0",
        "eslint": "8.3.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "http-server": "14.0.0",
        "jest": "26.6.3",
        "jsdoc": "3.6.10",
        "jsdoc-to-markdown": "7.1.0",
        "prettier": "2.5.0"
    }
}
